{
    "images": {
        "driver": "File",
        "namespace": "cache-image", // tên thư mục cache
        "options": {
            "keepExt": true, // giữ lại đuôi file
            "hashFileName": true // encode tên file
        }
    },
    "translations": {
        "driver": "File",
        "namespace": "cache-translations", // tên thư mục cache
        "options": {
            "keepExt": true, // giữ lại đuôi file
            "hashFileName": true // encode tên file
        }
    },
    "redux-persist": {
        "driver": "LocalStore",
        "namespace": "redux-persist",
        "options": {},
        "keepClear": true
    },
    "auth": {
        "driver": "LocalStore",
        "namespace": "auth",
        "options": {},
        "keepClear": true
    },
    "stringee": {
        "driver": "LocalStore",
        "namespace": "stringee",
        "options": {},
        "keepClear": true
    }
